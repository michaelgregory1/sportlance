<div class="the-calendar">
  <h2 class="text-center"><strong>Your Schedule</strong></h2>
  <div class="switch-weeks">
    <%= link_to(change_calendar_backwards_path(:user => {"value" => current_user}, count: @modifier), { :method => :change_calendar_backwards, class: "switch-item"}) do %>
      <i class="fas fa-arrow-left"></i>
      <p>Previous week</p>
    <% end %>
    <p class="text-center">Click on a time-slot to adjust your availability</p>
    <%= link_to(change_calendar_forwards_path(:user => {"value" => current_user}, count: @modifier), { :method => :change_calendar_forwards, class: "switch-item"}) do %>
      <p>Next week</p>
      <i class="fas fa-arrow-right"></i>
    <% end %>
  </div>
  <div class="calendar-headers">
    <div class="calendar-header header-item flex-it curved-tl-border grid-time">Time</div>
    <div class="calendar-header header-item"><div class="flex-it"><strong><%= (Date.today + @modifier).beginning_of_day.strftime('%A') %></strong></div><div class="flex-it"><%= (Date.today + @modifier).beginning_of_day.strftime('%d-%m-%y') %></div></div>
    <div class="calendar-header header-item"><div class="flex-it"><strong><%= ((Date.today + @modifier) + 1).beginning_of_day.strftime('%A') %></strong></div><div class="flex-it"><%= ((Date.today + @modifier) + 1).beginning_of_day.strftime('%d-%m-%y') %></div></div>
